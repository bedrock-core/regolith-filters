{
  "$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
  "author": "DrAv0011",
  "name": "starter-example",
  "description": "Start example reference for regolith-filters_bundler",
  "packs": {
    "behaviorPack": "./packs/BP",
    "resourcePack": "./packs/RP"
  },
  "regolith": {
    "dataPath": "./packs/data",
    "formatVersion": "1.4.0",
    "filterDefinitions": {
      "bundler": {
        "url": "github.com/@bedrock-core/regolith-filters",
        "version": "1.0.0"
      }
    },
    "profiles": {
      "build": {
        "export": {
          "readOnly": true,
          "target": "local",
          "build": "standard"
        },
        "filters": [
          {
            "filter": "bundler",
            "settings": {
              "tsConfigPath": "./tsconfig.dev.json",
              "debug": true
            }
          }
        ]
      },
      "default": {
        "export": {
          "readOnly": false,
          "target": "development",
          "build": "standard"
        },
        "filters": [
          {
            "filter": "bundler"
          }
        ]
      }
    }
  }
}